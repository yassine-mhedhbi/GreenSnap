    "    numpy_img = mpimg.imread(picture_path) # or np.array(img)\n",
    "    img       = img.resize((48, 64), Image.ANTIALIAS)\n",
    "    \n",
    "    return np.array(img)\n",
    "\n",
    "def f(conv_nn, x):\n",
    "    return find_max(conv_nn.predict(np.array([x]))[0])"
